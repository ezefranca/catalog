<% include header %>
<% project = JSON.parse(project) %>
<div class="row-fluid">
  <div class="span3 white-background-submissions">
    <p class="student-deets">
      <strong><%= project.title %></strong><br />
      by <em><%= project.author %></em><br/><br/>
      <%= project.medium %><br/>
      <em><%= project.measurements %></em><br/><br/>
      <a href="#" target="_blank"><%= project.website %></a></p>
  </div>
    <div class="span9">
      <% for (var i = 0; i < project.assets.length; i++) { %>
        <% if (project.assets[i].type != "video") { %>
          <img class="hires portos" alt="A landscape treatment of the logo You Are Here for the Exhibition" src="//:0" data-src="<%= project.assets[i].url %>" width="100%" />
        <% } %>
      <% } %>
      <% for (var j = 0; j < project.assets.length; j++) { %>
        <% if (project.assets[j].type == "video") { %>
          <div class="container-video">
              <div class="vendor">
                  <iframe src="http://player.vimeo.com/video/<%= project.assets[j].url.substring(project.assets[j].url.lastIndexOf('/') + 1) %>?title=0&amp;byline=0&amp;portrait=0" width="970" height="546" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>
              </div>
          </div>
        <% } %>
      <% } %>
    </div>
  </div>
</div>

<% include footer %>