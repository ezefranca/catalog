<% include header %>

  <div class="row-fluid">
    <% if (!projects.length) { %>
    <p class="white-background-submissions">No submissions are currently available. Check again later!</p>
    <% } %>

    <ul id="tiles">
      <!-- These are our grid blocks -->
      <%  for (var i = 0; i < projects.length; i++) { %>
        <li>
            <a href="/<%= projects[i].year %>/project/<%= projects[i].id %>/<%= projects[i].degree.replace(/ /g,'-') %>/<%= projects[i].author.toLowerCase().replace(/ /g,'-')
      .replace(/[^\w-]+/g,'') %>">

          <% for (var j = 0; j < projects[i].assets.length; j++) { %>
            <% if (projects[i].assets[j].type === "image") { %>
            <img src="<%= projects[i].assets[j].thumbnailurl %>" alt="<%= projects[i].title %>">
            <% break; } %>
          <% } %>

            </a>
            <p>
              <strong>
                <a href="/<%= projects[i].year %>/project/<%= projects[i].id %>/<%= projects[i].degree.replace(/ /g,'-') %>/<%= projects[i].author.toLowerCase().replace(/ /g,'-')
      .replace(/[^\w-]+/g,'') %>"><%= projects[i].title %></a>
              </strong><br />
            <em>
              <a href="/<%= projects[i].year %>/project/<%= projects[i].id %>/<%= projects[i].degree.replace(/ /g,'-') %>/<%= projects[i].author.toLowerCase().replace(/ /g,'-')
      .replace(/[^\w-]+/g,'') %>"><%= projects[i].author %></a>
            </em>
          </p>
        </li>
      <% } %>
      <!-- End of grid blocks -->
    </ul>
  </div><!-- End Row-Fluid -->

<% include footer %>
