<% include header %>
<% projects = JSON.parse(projects) %>

    <div class="row-fluid">
      <ul id="tiles">
        <!-- These are our grid blocks -->
        <%  for (var i = 0; i < projects.length; i++) { %>
          <li>
              <% for (var j = 0; j < projects[i].assets.length; j++) { %>
                  <% if (projects[i].assets[j].type != "video") { %>
                      <a href="/project/<%= projects[i].id %>"><img src="<%= projects[i].assets[j].thumbnailurl %>" alt="<%= projects[i].title %>"></a>
                  <% break; } %>
              <% } %>
          <p><strong><a href="/project/<%= projects[i].id %>"><%= projects[i].title %></a></strong><br />
          <em><a href="/project/<%= projects[i].id %>"><%= projects[i].author %></a></em></p></li>
        <% } %>
        <!-- End of grid blocks -->
      </ul>
</div><!-- End Row-Fluid -->

<% include footer %>
